# routes

# misc.
#
*	/		module:secure
GET	/public/	staticDir:public
GET	/favicon.ico	404

# user
#
GET	/User/?			ESECtlUser.lsUsers
GET /User/lsgroups/?	ESECtlUser.lsGroups
GET /User/lsgroups/listusersofgroup/?	ESECtlUser.listUsersOfGroup
POST /User/lsgroups/?	ESECtlUser.addUserToGroup
GET	/User/add/?		ESECtlUser.addUser
POST	/User/add/?		ESECtlUser.addUserPost
GET	/User/mod/?		ESECtlUser.addUser
GET	/User/del/?		ESECtlUser.delUser
GET	/User/addgroup/?	ESECtlUser.addGroup
POST	/User/addgroup/?	ESECtlUser.addGroupPost

# home
#
GET	/							ESECtlUser.lsCalendars
GET	/{uri_user}/?						ESECtlUser.lsCalendars
GET	/{uri_user}/{uri_cal}/?					ESECalendarController.lsEvents
GET	/{uri_user}/{uri_cal}/add/?				ESECalendarController.addEvent
POST	/{uri_user}/{uri_cal}/add/?				ESECalendarController.addEventPost
GET	/{uri_user}/{uri_cal}/mod/?				ESECalendarController.modEvent
GET	/{uri_user}/{uri_cal}/del/?				ESECalendarController.delEvent
GET	/{uri_user}/{uri_cal}/{uri_yy}/{uri_mm}/{uri_dd}/?	ESECalendarController.lsEvents
